{
  "name": "@nchlswhttkr/hugo-proxy",
  "version": "0.1.0",
  "description": "Transforms and caches responses from various sites (Bandcamp, YouTube, Twitter, etc...) for embedded content on my website",
  "scripts": {
    "build": "source build.sh"
  },
  "license": "MIT",
  "private": true,
  "devDependencies": {
    "prettier": "^2.1.2"
  },
  "dependencies": {
    "wrangler": "^2.0.16",
    "esbuild": "^0.14.43",
    "eslint": "^8.19.0"
  },
  "eslintConfig": {
    "env": {
      "browser": true,
      "commonjs": true,
      "es2021": true
    },
    "extends": "eslint:recommended",
    "parserOptions": {
      "ecmaVersion": "latest"
    },
    "rules": {},
    "globals": {
      "CACHED_MEDIA": true,
      "CACHED_RESPONSES": true,
      "VIMEO_SECRET_KEY": true,
      "WRITER_SECRET": true,
      "YOUTUBE_SECRET_KEY": true
    }
  }
